Box}\n        />\n        <AppRoute\n          exact\n          path={\"/searchx/\" + query}\n          layout={_Guest}\n          auth={Guest}\n          component={SearchMain}\n        />\n        <AppRoute\n          exact\n          // path={\"/*\"}\n          layout={_Guest}\n          auth={Guest}\n          component={NotFound}\n        />\n      </Switch>\n    </section>\n  );\n};\nconst mapStateToProps = (state) => {\n  console.log(state, \"bosku\")\n  return {\n    id: state.movie.id,\n    query: state.movie.query\n  };\n}\nconst mapDispatchToProps = (dispatch) => {};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Routesx);\n","/Users/spacetroopers/mini_project/movie_project/src/pages/detail_movie/overview.js",[],"/Users/spacetroopers/mini_project/movie_project/src/components/Overview/index.js",["334","335"],"import React from \"react\";\nimport Carousel from \"components/Homepage/carousel\";\nimport Category from \"components/Homepage/category\";\nimport Main from \"components/Overview/parts/main\";\n\nconst Overview = (props) => {\n  console.log(props,\"testx\")\n  return (\n    <div>\n      <Main dummy={props.dummy} ></Main>\n    </div>\n  );\n};\n\nexport default Overview;\n","/Users/spacetroopers/mini_project/movie_project/src/components/Overview/parts/main.js",[],"/Users/spacetroopers/mini_project/movie_project/src/pages/detail_movie/index.js",["336","337","338"],"import React, {useContext, useEffect} from 'react';\nimport Character from 'pages/detail_movie/character'\nimport Overview from 'pages/detail_movie/overview'\nimport Review from 'pages/detail_movie/review'\nimport Banner from \"components/Layout/banner\";\nimport Category from \"components/Homepage/category\";\nimport { darkMode } from \"provider/darkmode\";\nimport { Overviewx } from \"data_dummy/overview\";\nimport { connect } from 'react-redux';\nimport Card from \"components/Homepage/card\";\nimport Page from \"components/Homepage/page_nation\";\nimport {\n  MOVIE_ID\n} 